<metadata xml:lang="en"><Esri><CreaDate>20160513</CreaDate><CreaTime>10582000</CreaTime><ArcGISFormat>1.0</ArcGISFormat><SyncOnce>TRUE</SyncOnce><ModDate>20160610</ModDate><ModTime>17281600</ModTime><scaleRange><minScale>150000000</minScale><maxScale>5000</maxScale></scaleRange><ArcGISProfile>ItemDescription</ArcGISProfile></Esri><tool name="CreateHANDMosaicDataset" displayname="Create HAND Mosaic Dataset" toolboxalias="RAPID Tools" xmlns=""><arcToolboxHelpPath>c:\program files (x86)\arcgis\desktop10.5\Help\gp</arcToolboxHelpPath><parameters><param name="in_output_location" displayname="Output Location" type="Required" direction="Input" datatype="Workspace" expression="in_output_location"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The path to the geodatabase.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="in_mosaic_dataset_name" displayname="Mosaic Dataset Name" type="Required" direction="Input" datatype="String" expression="in_mosaic_dataset_name"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Name the mosaic dataset you are creating.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="in_HAND_raster_dataset" displayname="HAND Raster Dataset" type="Required" direction="Input" datatype="Raster Dataset" expression="in_HAND_raster_dataset"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Input of HAND (Height Above the Nearest Drainge) raster&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference></param><param name="start_date_time" displayname="Start Date and Time" type="Required" direction="Input" datatype="Date" expression="start_date_time"><dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;The input starting date and time of  the RAPID discharge result.&lt;/S